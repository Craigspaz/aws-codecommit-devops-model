{
  "name": "aws-codecommit-devops-model",
  "version": "1.0.0",
  "description": "AWS CodeCommit based devops practice at scale.",
  "bin": {
    "codecommit-devops-model": "bin/codecommit-devops-model.js"
  },
  "scripts": {
    "build": "tsc",
