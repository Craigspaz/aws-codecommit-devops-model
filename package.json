    "jest": "^25.5.4",
    "ts-jest": "^25.5.1",
    "ts-node": "^8.10.2",
    "typescript": "~3.7.2"
  },
  "dependencies": {
    "@aws-cdk/aws-codebuild": "^1.54.0",
    "@aws-cdk/aws-codecommit": "^1.54.0",
    "@aws-cdk/aws-events-targets": "^1.54.0",
    "@aws-cdk/aws-iam": "^1.54.0",
    "@aws-cdk/aws-lambda-nodejs": "^1.54.0",
    "@aws-cdk/core": "1.54.0",
    "parcel-bundler": "^1.12.4",
    "source-map-support": "^0.5.19"
  },
